<%- include ('header-client') %>
<% if(profile){%>
<div class="profile">
  <!-- thanks -->
  
  <div class="change-profile">
    <p class="about-me"><%=profile.other.about_me%></p>
  </div>

  <div class="wrap">
    <div class=" column-info">

      <div class="col-1 name">
        <a class="hire">Hire</a>
        <p class="name"><%=profile.name%></p>
        
        <p style="color: red;"><%=profile.other.country%></p>
        <p class=""><%=profile.other.email%></p>
        
      </div>

      <div class="col-1 skills">
        <p class="h">Skills</p>
        
        <div class="skill">
          <% if (profile.other.skill != []){%>
          <% profile.other.skill.forEach(function(sk){%>
              <p><%=sk%></p>
            <%});%>
          <%}%>
        </div>
        
      </div>

      <div class="col-1 complete_projects">
        <p class="h">Completed Projects</p>
        
        <div class="projects">
        <% if (profile.other.completed_projects != []){%>
          <% profile.other.completed_projects.forEach(function(pr){%>
            <p><%=pr%></p>
          <%});%>
          <%}%>
        </div>
        
        
      </div>

      <div class="col-1 edu-level">
        <p class="h">Education</p>
        <div>
          <%=profile.other.education_level%>
        </div>
      </div>

    </div>

    <div class="more-info">
      <div class="col-2 bank">
        <p class="h1">Bank Account</p>
        <p class="bank-num"><%=profile.other.bank_account%></p>
      </div>
      <div class="col-2 exp">
        <p class="h1">Experience</p>
        <p><%=profile.other.experience%></p>
      </div>
      <div class="col-2 deal">
        <p class="h1">Price</p>
        <p style="color: rgb(255, 81, 0);"><%=profile.other.price%> $/h</p>
      </div>
    </div>
  </div>
</div>
</div>
<%}%>

<%- include ('footer') %>
